<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandTextView">
        <attr name="icon_width" format="dimension" />
        <attr name="icon_height" format="dimension" />
        <attr name="icon_background" format="reference" />
        <attr name="space_number" format="integer" />
        <attr name="max_rows" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExpandView">
        <attr name="iconDrawable" format="reference" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="expandBackground" format="reference" />
        <attr name="isNeedEllipsis" format="boolean" />
        <attr name="isNeedShowIcon" format="boolean" />
        <attr name="maxRows" format="integer"/>
        <attr name="textInnerPadding" format="dimension"/>
    </declare-styleable>
</resources>